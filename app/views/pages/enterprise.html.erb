<%= content_for(:title)       { t ".title" }       %>
<%= content_for(:description) { t ".description" } %>
<%= content_for(:image_url) { image_url 'social/enterprise_card.png' } %>

<div class="banner-enterprise" style="background-image: linear-gradient(90deg, rgba(0,0,0,0.95) 0%, rgba(0,0,0,0.1) 80%), url(<%= image_path 'officials/students-6.jpg' %>)">
  <div class="banner-content">
    <h1>
      <%= t('.tagline_html', :default => 'Re-invent work <br>in a digital world.').html_safe %>
    </h1>
    <p>
      <%= t('.tagline_details_html', :default => 'Learn the technical skills to change the way you work <br>in your organisation.
      <strong>Work faster, be more efficient.</strong>').html_safe %>
    </p>
    <a class="typeform-share button btn btn-danger" href="https://lewagon.typeform.com/to/LOs0uR" data-mode="popup" target="_blank" > <%= t('.apply', :default => 'Enquire Now') %> </a>
  </div>
</div>

